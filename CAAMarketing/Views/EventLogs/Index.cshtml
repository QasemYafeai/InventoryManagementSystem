@model IEnumerable<EventLog>

<div class="container-fluid">
        <div class="table-responsive">
            <div class="table-wrapper">
                <div class="table-title d-flex justify-content-between align-items-center">
                <h1>Event logs</h1>
                <div class="col-xs-7">
                    <a asp-controller="Inventories" asp-action="InventoryEventsReport" title="Click here to generate inventory events report" class="btn btn-primary btn-color-Create"> Inventory Events Report</a>
                </div>
                </div>
                <table class="table table-hover">
                    <thead class="thead-light">
        <tr>
            <th>Event Name</th>
            <th>Item Name</th>
            <th>Quantity</th>
            <th>Log Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.EventName</td>
                <td>@item.ItemName</td>
                <td>@item.Quantity</td>
                <td>@item.LogDate</td>
            </tr>
        }
    </tbody>
</table>
        </div>
    </div>
</div>
